edit_icon = WebDriverWait(driver, 10).until(
    EC.presence_of_element_located((By.ID, "GLSEditSpecViewAttributeValue"))
)
actions.move_to_element(edit_icon).click().perform()
print("Clicked Edit icon")
time.sleep(20)


