display_input = WebDriverWait(driver, 10).until(
    EC.presence_of_element_located((By.NAME, "TypeActualDisplay"))
)
display_input.clear()
display_input.send_keys("Packaging Component")
display_input.send_keys(Keys.RETURN)
hidden_input = driver.find_element(By.NAME, "TypeActual")
driver.execute_script("arguments[0].value = arguments[1];", hidden_input, "Packaging Component")
