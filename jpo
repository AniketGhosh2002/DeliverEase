somehow it working like clicked again cell reveals the dropdown but Im not able click any deopdown element and clicking on textbox shows the cursor in it but failed to write things



import time
from selenium.webdriver.common.by import By
from selenium.webdriver.support.ui import WebDriverWait
from selenium.webdriver.support import expected_conditions as EC

def wait_for_table(driver, timeout=30):
    try:
        print("‚è≥ Waiting for spinner to disappear...")
        WebDriverWait(driver, timeout).until(
            EC.invisibility_of_element_located((By.ID, "spinner_div"))
        )
        print("‚úÖ Spinner gone.")

        print("‚è≥ Waiting for table body to be present...")
        table = WebDriverWait(driver, timeout).until(
            EC.presence_of_element_located((By.ID, "bodyTable"))
        )
        print("‚úÖ Found bodyTable.")

        time.sleep(3)  # let rows settle
        rows = table.find_elements(By.TAG_NAME, "tr")
        print(f"‚úÖ Found {len(rows)} rows inside bodyTable.")
        return rows
    except Exception as e:
        print("‚ùå Timeout or error while waiting:", str(e))
        return []

def fill_cell(driver, cell, value, is_dropdown=False):
    try:
        driver.execute_script("arguments[0].scrollIntoView(true);", cell)
        time.sleep(1)
        print(f"üëâ Clicking into cell for value '{value}'...")
        actions.move_to_element(cell).click().perform()
        time.sleep(3)
        actions.move_to_element(cell).click().perform()
        time.sleep(3)

        if is_dropdown:
            actions.move_to_element(cell).click().perform()
            time.sleep(3)
            actions.move_to_element_with_offset(cell, 0, 30).click().perform()
        else:
            cell.send_keys(str(value))

    except Exception as e:
        print(f"‚ùå Error while filling cell with '{value}':", str(e))


# === Example usage ===
rows = wait_for_table(driver)
if rows:
    first_row = rows[1]   # assume data starts at row[1]
    cells = first_row.find_elements(By.TAG_NAME, "td")
    print(f"Row has {len(cells)} cells.")

    # 1st cell ‚Üí dropdown
    fill_cell(driver, cells[0], "Tape - Spool - Female", is_dropdown=True)
    time.sleep(3)
    fill_cell(driver, cells[1], "Tape - Spool - Female", is_dropdown=True)
    time.sleep(3)
    # 3rd cell ‚Üí textbox
    fill_cell(driver, cells[2], 100, is_dropdown=False)
